&lt;span&gt;{ZL:CutText({Field="synopsis"/},@SynNum,...)/}&lt;/span&gt;
&lt;p&gt;&lt;span&gt;{Field="type"/}&lt;/span&gt;&lt;/p&gt;
&lt;/div&gt;
&lt;/li&gt;
{/Repeate}
&lt;/ul&gt;
&lt;div class="clearfix"&gt;&lt;/div&gt;
{ZL.Page id="响应式列表分页v3"/}</LabelContent>
    <LabelCount>@ShowNum</LabelCount>
