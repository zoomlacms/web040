<?xml version="1.0" standalone="yes"?>
<NewDataSet>
  <Table>
    <LabelID>335</LabelID>
    <LabelName>输出首页施工队伍</LabelName>
    <LabelType>4</LabelType>
    <LabelCate>建途专用</LabelCate>
    <LabelDesc>输出首页施工队伍</LabelDesc>
    <LabelParam>NodeID,1,1,节点ID|ShowNum,5,1,显示数|TitleNum,40,1,标题长</LabelParam>
    <LabelTable>ZL_CommonModel LEFT JOIN ZL_C_Constructionteam ON ZL_CommonModel.ItemID=ZL_C_Constructionteam.ID</LabelTable>
    <LabelField>ZL_CommonModel.NodeID,ZL_CommonModel.GeneralID,ZL_CommonModel.Title,ZL_C_Constructionteam.lx,ZL_C_Constructionteam.region,ZL_C_Constructionteam.time</LabelField>
    <LabelWhere>(NodeID=@NodeID or FirstNodeID=@NodeID) and status=99 And ModelID=49 And dsfz like '%{$GetUrldecode({$GetRequest(city)$})$}%'</LabelWhere>
    <LabelOrder>ZL_CommonModel.CreateTime DESC</LabelOrder>
    <LabelContent>&lt;div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 one_center"&gt;
&lt;div class="table-responsive"&gt;
&lt;table class="table table-hover"&gt;
&lt;thead&gt;
&lt;tr&gt;
&lt;th&gt;类型&lt;/th&gt;
&lt;th&gt;标题&lt;/th&gt;
&lt;th class="text_center"&gt;发布时间&lt;/th&gt;
&lt;th class="text_center"&gt;发布城市&lt;/th&gt;
&lt;/tr&gt;
&lt;/thead&gt;
{Repeate}
&lt;tr&gt;
&lt;th scope="row" class="home_tha"&gt;&lt;a href="{ZL:GetInfoUrl({Field="GeneralID"/})/}" target="{ZL:GetInfoOpen({Field="NodeID"/})/}"&gt;{Field="lx"/}&lt;/a&gt;&lt;/th&gt;
&lt;td&gt;&lt;a href="{ZL:GetInfoUrl({Field="GeneralID"/})/}" title="{Field="Title"/}" target="{ZL:GetInfoOpen({Field="NodeID"/})/}"&gt;{ZL:CutText({Field="Title"/},@TitleNum,...)/}&lt;/a&gt;&lt;/td&gt;
&lt;td class="text_center"&gt;{ZL:FormatDate({Field="time"/},yyyy-MM-dd)/}&lt;/td&gt;
&lt;td class="text_center"&gt;{Field="region"/}&lt;/td&gt;
&lt;/tr&gt;{/Repeate}
&lt;/table&gt;
&lt;/div&gt;
&lt;/div&gt;</LabelContent>
    <LabelCount>@ShowNum</LabelCount>
    <LabelAddUser>0</LabelAddUser>
    <LabelNodeID>0</LabelNodeID>
    <Modeltypeinfo>计数判断</Modeltypeinfo>
    <addroot>循环计算</addroot>
    <setroot>等于</setroot>
    <Modelvalue />
    <Valueroot />
    <IsOpen>0</IsOpen>
    <FalseContent />
    <DataSourceType>{"ds_m":"main","ds_s":"main","tb_m":"ZL_CommonModel","tb_s":"ZL_C_Constructionteam"}</DataSourceType>
    <ConnectString />
    <ProceName />
    <ProceParam />
  </Table>
</NewDataSet>