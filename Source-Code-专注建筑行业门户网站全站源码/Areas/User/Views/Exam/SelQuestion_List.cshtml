@using System.Data
@model ZoomLa.SQLDAL.SQL.PageSetting
@{  Model.cols = 4;
    Model.url = "SelQuestion_Date";
    DataTable dt = new DataTable();
}
<table class="table table-striped table-bordered table-hover"  >
    <tr>
        <td>选择</td>
        <td>试题标题</td>
        <td>类别</td>
        <td>题型</td>
    </tr>
    @foreach(DataRow dr in Model.dt.Rows)
    {
        <tr>
        <td> <%#GetCheck() %></td>
        <td><button id="upd" value="@dr["p_id"]">@dr["p_title"]</button></td>
        <td> <%#GetClass(@dr["p_Class"])></td>
        <td>  <%#GetType(@dr["p_Type"])%></td>
    </tr> 
    }
    @Html.Partial("_page", Model)
</table>
