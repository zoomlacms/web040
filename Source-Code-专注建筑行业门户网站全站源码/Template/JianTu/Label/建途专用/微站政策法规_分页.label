<?xml version="1.0" standalone="yes"?>
<NewDataSet>
  <Table>
    <LabelID>445</LabelID>
    <LabelName>微站政策法规_分页</LabelName>
    <LabelType>4</LabelType>
    <LabelCate>建途专用</LabelCate>
    <LabelDesc />
